extends layout

block content

	div.col-xs-10.col-xs-offset-1(style="text-position: center; background-color:#FFFFFF; opacity:1; border-radius: 20px 20px 20px 20px; padding-bottom:10px; padding-top:10px;")
		
		hr
		h1(align="center", style="color: #000000; font-family: 'Lato', sans-serif;") Select Student
		hr
		center
			

		each member in members
			.col-xs-10.class-select-box.col-xs-offset-1
				.col-xs-6
					h3 #{member.name}
				.col-xs-6
					a(href="/zapp/primary/login/student/#{member.id}", style="float: right; margin-top: 25px;").btn.btn-primary Select			
	p &nbsp;
