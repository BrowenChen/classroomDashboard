doctype 5
html(ng-app='developerApp')
	head

		meta(name='viewport', content='width=device-width, initial-scale=1.0')
		meta(name='description', content='')
		meta(name='author', content='templatemo')
		meta(charset='UTF-8')

		block js
		script(src='http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js')
		script(src='https://ajax.googleapis.com/ajax/libs/angularjs/1.0.7/angular.min.js')
		script(src='/js/bootstrap.js')
		script(src='/js/docController.js')
		
		link(href='/css/bootstrap.css', rel='stylesheet')
		link(href='/css/shop-homepage.css', rel='stylesheet')
		
	body
		nav.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
			.container
	
				.navbar-header
					button.navbar-toggle(type='button', data-toggle='collapse', data-target='#bs-example-navbar-collapse-1')
						span.sr-only Toggle navigation
						span.icon-bar
						span.icon-bar
						span.icon-bar
					a.navbar-brand Zzish
	
				#bs-example-navbar-collapse-1.collapse.navbar-collapse
					ul.nav.navbar-nav.navbar-right
						li
							a(href='/products') Products
						li
							a(href='/prices') Prices
						li
							a(href='/docs') Docs
						li          
							a(href='/help') Help
						li
							a(href='http://blog.zzish.com') Blog
						li
							a.btn Login
						li
							a.btn Sign up
	

		.container
			.row
				.col-md-3(ng-controller="DocNavigationController")
					
					.list-group
						div.list-group-item.list-group-heading Product Docs
						div.list-group-item(href="/docs/product/core") Core
						div.list-group-item(ng-show="#{nav.productCore}")
							p
								a.list-group-subitem(href='#') Users and Relationships
							p
								a.list-group-subitem(href='#') Activities and Actions

						div.list-group-item(href="/docs/product/personal") Personal
						div.list-group-item(ng-show="#{nav.productPersonal}")
							p
								a.list-group-subitem(href='#') Adaptive Algorithms
							p
								a.list-group-subitem(href='#') Learning Analytics

						div.list-group-item(href="/docs/product/social") Social
						div.list-group-item(ng-show="#{nav.productSocial}")
							p
								a.list-group-subitem(href='#') Gamification
							p
								a.list-group-subitem(href='#') Classroom Ready

					.list-group
						div.list-group-item.list-group-heading SDKs
						div.list-group-item(href="/docs/sdk/ios")  iOS
						div.list-group-item(ng-show="#{nav.sdkIos}")
							p
								a.list-group-subitem(href='#') Guide
							p
								a.list-group-subitem(href='#') API Reference

						div.list-group-item(href="/docs/sdk/android") Android
						div.list-group-item(ng-show="#{nav.sdkAndroid}")
							p
								a.list-group-subitem(href='#') Guide
							p
								a.list-group-subitem(href='#') API Reference

						div.list-group-item(href="/docs/sdk/rest") REST
						div.list-group-item(ng-show="#{nav.sdkRest}")
							p
								a.list-group-subitem(href='#') Guide
							p
								a.list-group-subitem(href='#') API Reference
						

					.list-group
						div.list-group-item.list-group-heading(href='#') APIs
						div.list-group-item(href="/docs/api/apirest") Rest API
						div.list-group-item(ng-show="#{nav.api}")
							p
								a.list-group-subitem(href='#') Guide
							p
								a.list-group-subitem(href='#') Swagger Console

				.col-md-9
					block content

